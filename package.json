{
  "name": "supa",
  "version": "0.0.1",
  "description": "JavaScript Framework without the vdom",
  "main": "./dist/supa.common.js",
  "module": "./dist/supa.esm.js",
  "unpkg": "./dist/supa.js",
  "jsdelivr": "./dist/supa.js",
  "files": [
    "src",
    "dist/*.js"
  ],
  "scripts": {
    "lint": "eslint src/*.js test/**/*.js && prettier --write \"{./test/*.js,./test/**/*.js,./src/*.js,./src/**/*.js}\"",
    "build": "npm run lint && rollup --config",
    "test": "ava"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jschatz1/supa.git"
  },
  "keywords": [
    "retur",
    "data",
    "schema"
  ],

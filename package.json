  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jschatz1/supa/issues"
  },
  "homepage": "https://github.com/jschatz1/supa#readme",
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "ava": "^2.1.0",
    "babel-eslint": "^10.0.2",
    "eslint": "^6.0.1",
    "prettier": "^1.18.2",
    "rollup": "^1.16.4",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-uglify": "^6.0.2"
  },
  "dependencies": {}
